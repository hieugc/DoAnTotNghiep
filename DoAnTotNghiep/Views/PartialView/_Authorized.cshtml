<script src="~/js/Chat/connect.js" type="text/javascript"></script>

<script type="text/javascript">
    connection.on("@(DoAnTotNghiep.Enum.TargetSignalR.Connect() + "-" + (Model == null ? string.Empty : Model.UserAccess))", function (message) {
