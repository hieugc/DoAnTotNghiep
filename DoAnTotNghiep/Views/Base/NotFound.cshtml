@model IEnumerable<DoAnTotNghiep.Entity.AdminReport>
@{
    ViewData["Title"] = "404 - NotFound";
    Layout = "_LayoutEmpty";
}

<!-- Error -->
<div class="container-xxl container-p-y">
    <div class="misc-wrapper">
        <h2 class="mb-2 mx-2">Page Not Found :(</h2>
        <p class="mb-4 mx-2">Oops! 😖 The requested URL was not found on this server.</p>
        <a href="@Url.Action("Index", "Home")" class="btn btn-primary">Quay về trang chủ</a>
        <div class="mt-3">
            <img src="~/Image/page-misc-error-light.png"
                 alt="page-misc-error-light"
                 width="500"
                 class="img-fluid"
                 data-app-dark-img="illustrations/page-misc-error-dark.png"
                 data-app-light-img="~/Image/page-misc-error-light.png" />
        </div>
    </div>
</div>
<!-- /Error -->