            <td>
                @Html.DisplayFor(modelItem => item.Users.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Citys.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Districts.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wards.Name)
            </td>
            <td>
